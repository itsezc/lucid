{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
      "lib": [
          "ES2022",
      ],
      "types": [
          "bun-types",
          "node"
      ],
      "target": "ES2022",
      "module": "ES2022",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "experimentalDecorators": true,
      "moduleResolution": "node",
      "strict": true,
      "skipLibCheck": false
  }
}
