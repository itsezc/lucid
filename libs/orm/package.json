{
  "name": "@surreal-tools/orm",
  "version": "0.1.0",
  "main": "src/index.ts",
  "dependencies": {
    "cmd-ts": "^0.12.0",
    "fs": "0.0.1-security",
    "isomorphic-ws": "^5.0.0",
    "packemon": "^2.4.1"
  },
  "scripts": {
    "build": "packemon build",
    "pack": "packemon pack --addEngines --addFiles --declaration"
  },
  "packemon": {
    "format": "lib",
    "inputs": {
      "index": "src/schema/entry.ts"
    },
    "platform": "node"
  },
  "files": [
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}"
  ]
}
