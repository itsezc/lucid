{
	"extends": "../tsconfig.json",
	"include": [
		"./src/**/*"
	],
	"compilerOptions": {
		"rootDir": "./src",
		"lib": [
			"ES2022"
		],
		"module": "ES2022",
		"target": "ESNext",
		"strictPropertyInitialization": false,
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"resolveJsonModule": true,
	}
}